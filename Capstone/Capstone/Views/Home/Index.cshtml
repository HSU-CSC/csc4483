@model Capstone.Models.Home.ImageMap

@{ 
    var imagePath = "/Resources/Images/" + Model.ImageFile;
}

<img src="@imagePath" usemap="#map"/>

<map name="map">
    @foreach(var r in Model.Regions)
    {
        var coords = r.TopLeftX + "," + r.TopLeftY + "," + r.BottomRightX + "," + r.BottomRightY;

        <area shape="rect" coords="@coords" href="@r.Url" alt=""/>
    }
</map>